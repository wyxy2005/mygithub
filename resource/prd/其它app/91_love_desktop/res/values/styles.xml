<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Theme" parent="@android:style/Theme.Wallpaper">
        <item name="android:windowNoTitle">true</item>
    </style>
    <style name="AnimationPreview">
        <item name="android:windowEnterAnimation">@anim/fade_in_fast</item>
        <item name="android:windowExitAnimation">@anim/fade_out_fast</item>
    </style>
    <style name="WorkspaceIcon">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:background">@drawable/shortcut_selector</item>
        <item name="android:paddingLeft">5.0dip</item>
        <item name="android:paddingRight">5.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_marginLeft">10.0dip</item>
        <item name="android:layout_marginRight">10.0dip</item>
        <item name="android:singleLine">true</item>
        <item name="android:shadowColor">#ff000000</item>
        <item name="android:shadowDy">1.0</item>
        <item name="android:shadowRadius">2.0</item>
        <item name="android:drawablePadding">3.0dip</item>
    </style>
    <style name="WorkspaceIcon.Portrait" parent="@style/WorkspaceIcon">
        <item name="android:paddingTop">4.0dip</item>
        <item name="android:layout_marginLeft">3.0dip</item>
        <item name="android:layout_marginTop">13.0dip</item>
        <item name="android:layout_marginRight">3.0dip</item>
        <item name="android:layout_marginBottom">0.0dip</item>
        <item name="android:drawablePadding">0.0dip</item>
    </style>
    <style name="WorkspaceIcon.Landscape" parent="@style/WorkspaceIcon">
        <item name="android:paddingTop">2.0dip</item>
        <item name="android:layout_marginLeft">10.0dip</item>
        <item name="android:layout_marginRight">10.0dip</item>
        <item name="android:drawablePadding">2.0dip</item>
    </style>
    <style name="SearchButton" parent="@android:style/Widget.Button.Small">
        <item name="android:paddingLeft">10.0dip</item>
        <item name="android:paddingTop">7.0dip</item>
        <item name="android:paddingRight">10.0dip</item>
        <item name="android:paddingBottom">9.0dip</item>
    </style>
    <style name="HotseatButton">
        <item name="android:background">@drawable/hotseat_bg_center</item>
        <item name="android:paddingLeft">12.0dip</item>
        <item name="android:paddingRight">12.0dip</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:scaleType">center</item>
    </style>
    <style name="HotseatButton.Left" parent="@style/HotseatButton">
        <item name="android:background">@drawable/hotseat_bg_left</item>
        <item name="android:layout_marginLeft">4.0dip</item>
    </style>
    <style name="HotseatButton.Right" parent="@style/HotseatButton">
        <item name="android:background">@drawable/hotseat_bg_right</item>
        <item name="android:layout_marginRight">4.0dip</item>
    </style>
    <style name="TipDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
    </style>
    <style name="PopupAnimation" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/popup_enter</item>
        <item name="android:windowExitAnimation">@anim/popup_exit</item>
    </style>
    <style name="setting_background">
        <item name="android:windowNoTitle">false</item>
        <item name="android:focusable">false</item>
        <item name="android:cacheColorHint">#00000000</item>
    </style>
    <style name="progressBarHorizontal_arabia" parent="@android:style/Widget.ProgressBar.Horizontal">
        <item name="android:maxHeight">4.0dip</item>
        <item name="android:indeterminateOnly">false</item>
        <item name="android:progressDrawable">@drawable/progress_horizontal_arabia</item>
        <item name="android:minHeight">4.0dip</item>
    </style>
    <style name="Theme.CustomDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>
    <style name="ShortCutMenuGrowFromBottom" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/popshortcutmenu_grow_from_bottom</item>
        <item name="android:windowExitAnimation">@anim/popshortcutmenu_shrink_from_top</item>
    </style>
    <style name="ShortCutMenuGrowFromBottomLeftToTopRight" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/popshortcutmenu_grow_from_bottomleft_to_topright</item>
        <item name="android:windowExitAnimation">@anim/popshortcutmenu_shrink_from_topright_to_bottomleft</item>
    </style>
    <style name="ShortCutMenuGrowFromBottomRightToTopLeft" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/popshortcutmenu_grow_from_bottomright_to_topleft</item>
        <item name="android:windowExitAnimation">@anim/popshortcutmenu_shrink_from_topleft_to_bottomright</item>
    </style>
    <style name="ShortCutMenuGrowFromTop" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/popshortcutmenu_grow_from_top</item>
        <item name="android:windowExitAnimation">@anim/popshortcutmenu_shrink_from_bottom</item>
    </style>
    <style name="ShortCutMenuGrowFromTopLeftToBottomRight" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/popshortcutmenu_grow_from_topleft_to_bottomright</item>
        <item name="android:windowExitAnimation">@anim/popshortcutmenu_shrink_from_bottomright_to_topleft</item>
    </style>
    <style name="ShortCutMenuGrowFromTopRightToBottomLeft" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/popshortcutmenu_grow_from_topright_to_bottomleft</item>
        <item name="android:windowExitAnimation">@anim/popshortcutmenu_shrink_from_bottomleft_to_topright</item>
    </style>
</resources>
